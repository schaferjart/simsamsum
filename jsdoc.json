{
    "source": {
        "include": [
            "src/js",
            "src/app.js",
            "README.md"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "./docs/",
        "recurse": true,
        "readme": "README.md",
        "template": "node_modules/docdash"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "default": {
            "outputSourceFiles": true
        }
    }
}